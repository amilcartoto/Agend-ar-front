[1mdiff --git a/src/components/carousel/Carousel.jsx b/src/components/carousel/Carousel.jsx[m
[1mnew file mode 100644[m
[1mindex 0000000..363573d[m
[1m--- /dev/null[m
[1m+++ b/src/components/carousel/Carousel.jsx[m
[36m@@ -0,0 +1,95 @@[m
[32m+[m[32mimport React, { useRef, useState, useEffect } from 'react';[m
[32m+[m[32mimport { Swiper, SwiperSlide } from 'swiper/react';[m
[32m+[m[32mimport 'swiper/css';[m
[32m+[m[32mimport 'swiper/css/grid';[m
[32m+[m[32mimport 'swiper/css/navigation';[m
[32m+[m[32mimport 'swiper/css/pagination';[m
[32m+[m[32mimport { Grid, Navigation, Pagination } from 'swiper/modules';[m
[32m+[m[32mimport Card from './Card';[m
[32m+[m[32mimport EventCard from './EventCard';[m
[32m+[m[32mimport { provincesData } from '@/data/provinces';[m
[32m+[m
[32m+[m
[32m+[m[32mconst Carousel = () => {[m
[32m+[m[32m  const provinces = Object.values(provincesData);[m
[32m+[m[32m  const events = provinces.flatMap(province => province.eventos || []);[m
[32m+[m[32m  const combinedData = [...provinces, ...events];[m
[32m+[m[41m  [m
[32m+[m[32m  const [swiper, setSwiper] = useState(null);[m
[32m+[m[32m  const navigationPrevRef = useRef(null);[m
[32m+[m[32m  const navigationNextRef = useRef(null);[m
[32m+[m[32m  const paginationRef = useRef(null);[m
[32m+[m
[32m+[m[32m  useEffect(() => {[m
[32m+[m[32m    if (swiper) {[m
[32m+[m[32m      swiper.params.navigation.prevEl = navigationPrevRef.current;[m
[32m+[m[32m      swiper.params.navigation.nextEl = navigationNextRef.current;[m
[32m+[m[32m      swiper.params.pagination.el = paginationRef.current;[m
[32m+[m
[32m+[m[32m      swiper.navigation.destroy();[m
[32m+[m[32m      swiper.navigation.init();[m
[32m+[m[32m      swiper.pagination.destroy();[m
[32m+[m[32m      swiper.pagination.init();[m
[32m+[m[32m      swiper.navigation.update();[m
[32m+[m[32m      swiper.pagination.update();[m
[32m+[m[32m    }[m
[32m+[m[32m  }, [swiper]);[m
[32m+[m
[32m+[m[32m  const paginationStyles = `[m
[32m+[m[32m    .swiper-pagination-bullet {[m
[32m+[m[32m      width: 20px;[m
[32m+[m[32m      height: 6px;[m
[32m+[m[32m      border-radius: 3px;[m
[32m+[m[32m      background: #ccc;[m
[32m+[m[32m      opacity: 1;[m
[32m+[m[32m      transition: all 0.2s ease-in-out;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    .swiper-pagination-bullet-active {[m
[32m+[m[32m      width: 30px;[m
[32m+[m[32m      background: orange;[m
[32m+[m[32m    }[m
[32m+[m[32m  `;[m
[32m+[m
[32m+[m[32m  return ([m
[32m+[m[32m    <div style={{ width: '60%', margin: '0 auto' }}>[m
[32m+[m[32m      <style>{paginationStyles}</style>[m
[32m+[m[32m      <div style={{ position: 'relative' }}>[m
[32m+[m[32m        <Swiper[m
[32m+[m[32m          modules={[Grid, Navigation, Pagination]}[m
[32m+[m[32m          onSwiper={setSwiper}[m
[32m+[m[32m          spaceBetween={5}[m
[32m+[m[32m          slidesPerView={4}[m
[32m+[m[32m          grid={{[m
[32m+[m[32m            rows: 2,[m
[32m+[m[32m            fill: "row",[m
[32m+[m[32m          }}[m
[32m+[m[32m          pagination={{ clickable: true, el: paginationRef.current }}[m
[32m+[m[32m          style={{[m
[32m+[m[32m            width: '100%',[m
[32m+[m[32m            '--swiper-grid-row-gap': '5px'[m
[32m+[m[32m          }}[m
[32m+[m[32m        >[m
[32m+[m[32m          {combinedData.map((item, index) => ([m
[32m+[m[32m            <SwiperSlide key={index}>[m
[32m+[m[32m              {item.nombre ? <Card province={item} /> : <EventCard evento={item} />}[m
[32m+[m[32m            </SwiperSlide>[m
[32m+[m[32m          ))}[m
[32m+[m[32m        </Swiper>[m
[32m+[m[32m        <div ref={navigationPrevRef} style={{ position: 'absolute', left: '-50px', top: '50%', transform: 'translateY(-50%)', cursor: 'pointer', zIndex: 10 }}>[m
[32m+[m[32m          <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" strokeWidth={1.5} stroke="currentColor" style={{width: '24px', height: '24px'}}>[m
[32m+[m[32m            <path strokeLinecap="round" strokeLinejoin="round" d="M15.75 19.5L8.25 12l7.5-7.5" />[m
[32m+[m[32m          </svg>[m
[32m+[m[32m        </div>[m
[32m+[m[32m        <div ref={navigationNextRef} style={{ position: 'absolute', right: '-50px', top: '50%', transform: 'translateY(-50%)', cursor: 'pointer', zIndex: 10 }}>[m
[32m+[m[32m          <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" strokeWidth={1.5} stroke="currentColor" style={{width: '24px', height: '24px'}}>[m
[32m+[m[32m            <path strokeLinecap="round" strokeLinejoin="round" d="M8.25 4.5l7.5 7.5-7.5 7.5" />[m
[32m+[m[32m          </svg>[m
[32m+[m[32m        </div>[m
[32m+[m[32m      </div>[m
[32m+[m[32m      <div ref={paginationRef} style={{ marginTop: '10px', textAlign: 'center', position: 'relative', zIndex: 10 }} />[m
[32m+[m[32m    </div>[m
[32m+[m[32m  );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mexport default Carousel;[m
